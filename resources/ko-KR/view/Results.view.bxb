
result-view {

  match: Results_post (this)

  message {

        template ("아래 학식 정보를 확인해 보세요")
  }

  render {
   
      if(size(this) == 1){
      layout{
        layout-macro (Listinfoone){
          param(info){
            expression (this)
          }
        }
        
        
      }
   
    }else{
      list-of (this){
        where-each (item){
          layout-macro (Listinfomany){
            param(info){
              expression (item)
            }
          }
        }
      }
    }
    
  }
    
  }
