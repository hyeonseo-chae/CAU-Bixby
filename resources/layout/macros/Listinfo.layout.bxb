layout-macro-def (Listinfo) {
  
  params {
    param (info) {
      type (Results_post)
      min (Optional) max (Many)
    }
  }
  
  content {
     cell-area {
      slot2 {
          content {
              order (PrimarySecondary)
              primary ("#{value(this.list.rest)}")
              secondary ("[#{value(this.list.time)}]")
            }
        }
       
     }
  }
}
